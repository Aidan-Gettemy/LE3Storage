%%
addpath funcs/
tablez = readtable("SensorDataT.txt");
%%
g = tablez.Properties.VariableNames;
for i = 1:266
    output = tablez(:,i).Variables;
    freqs(i,:) = calcfreq(output(end-160*60:end));
end
%%
i = 265;
ffs = freqs(i,:);
period1 = 1/ffs(1,1)
period2 = 1/ffs(1,2)

g{i}

output = tablez(:,i).Variables;
output = output(end-160*60:end);
Fs = 160;               % Sampling frequency                    
T = 1/Fs;             % Sampling period       
L = length(output);  % Length of signal
t = (0:L-1)*T;        % Time vector

figure
hold on
plot(t,output)

title("Original Data")
xlabel("t (seconds)")
ylabel("X(t)")

Y1 = fft(output);

% figure
% plot(Fs/L*(0:L-1),abs(Y1),"LineWidth",3)
% 
% title("Complex Magnitude of fft Spectrum")
% xlabel("f (Hz)")
% ylabel("|fft(X)|")

figure
P21 = abs(Y1/L);

P11 = P21(1:(L-1)/2+1);

P11(2:end-1) = 2*P11(2:end-1);

f = Fs/L*(0:(L/2));
plot(f,P11,"k","LineWidth",2) 
hold on

xlabel("f (Hz)")
ylabel("|P1(f)|")
%%
[B,I] = maxk(P11,10);
% if I(1) = 1; then go with the next peak...that is the dominant frequency
% if we use that index to index f
if I(1) < 3

    freq = f(I(2))
else
    freq = f(I(1))
end

period = 1/freq
